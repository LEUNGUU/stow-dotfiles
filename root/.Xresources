Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
xterm*termName: xterm-256color
xterm*faceName: Inconsolata:size=12:antialias=true
xter*fontName:xft:Inconsolata:size=13:antialias=true:autohint=true
xterm*locale: true
xterm*utf8: 1
XTerm*VT100.translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)
URxvt*font:xft:Inconsolata:size=13:antialias=true:autohint=true
URxvt*boldFont:xft:Inconsolata:weight=bold:size=13:antialias=true:autohint=true
URxvt*scrollBar: false
URxvt.perl-ext-common: default,clipboard
URxvt.allow_bold:true
URxvt*depth: 32
URxvt*buffered: true
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.secondaryWheel: 1
